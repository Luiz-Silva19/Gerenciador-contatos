@model ContatoModel
@{
    ViewData["Title"] = "Confirmar apagar contato";
}
<div class="text-center">
    <h3>Realmente deseja apagar (@Model.Name)?</h3>
    <a class="btn btn-primary" role="button" asp-controller="Contato" asp-action="Index">Cancelar</a>
    <a class="btn btn-danger" role="button" asp-route-id="@Model.Id" asp-controller="Contato" asp-action="Delete">Deletar</a>

</div>
